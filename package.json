{
  "name": "dovavision",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "6.11.3",
    "node": "12.11.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "gh-pages": "^2.1.1",
    "jquery": "^3.4.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "moment": "^2.24.0",
    "node-less-chokidar": "^0.4.1",
    "node-sass-chokidar": "^1.3.5",
    "react": "^16.8.6",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.8.6",
    "react-infinite-calendar": "^2.3.1",
    "react-moment": "^0.9.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "start": "npm-run-all -p sass start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar ./src/styles/sass/* -o  ./src/styles/css/",
    "sass": "node-sass-chokidar ./src/styles/sass/* -o ./src/styles/css/ --watch",
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
